  "private": true,
  "dependencies": {
    "@material-ui/core": "3.2.1",
    "@material-ui/icons": "3.0.1",
    "axios": "0.18.0",
    "bcrypt-nodejs": "0.0.3",
    "body-parser": "1.18.3",
    "cookie-parser": "1.4.3",
    "cors": "2.8.4",
    "csrf": "3.0.6",
    "dotenv": "6.1.0",
    "express": "4.16.4",
    "immutability-helper": "2.8.1",
    "jsonwebtoken": "8.3.0",
    "mongoose": "5.3.3",
    "multer": "1.4.1",
    "prop-types": "15.6.2",
    "react": "16.5.2",
    "react-dnd": "5.0.0",
    "react-dnd-html5-backend": "5.0.1",
    "react-dom": "16.5.2",
    "react-jss": "8.6.1",
    "react-redux": "5.0.7",
    "react-router": "4.3.1",
    "react-router-dom": "4.3.1",
    "redux": "4.0.1",
    "redux-thunk": "2.3.0",
    "uuid": "3.3.2"
  },
  "scripts": {
    "start-js": "react-scripts start",
    "start": "npm-run-all -p watch-css start-js",
    "server": "npm-run-all -p lint:watch webpack babel-node-js",
    "webpack": "webpack --watch",
    "flow": "flow check",
    "build": "npm run build-css && react-scripts build",
    "test": "jest --ci --verbose --forceExit --detectOpenHandles --coverage --env=jsdom",
    "test:dev": "jest --ci --verbose --env=jsdom --watch",
    "test:coverage": "cat ./coverage/lcov.info | coveralls",
    "babel-node-js": "nodemon --exec babel-node src/server/server.js",
    "lint": "esw webpack.config.* src --color",
    "lint:watch": "npm run lint -- --watch",
    "analyze": "webpack --profile --json > stats.json && webpack-bundle-analyzer stats.json -m server",
    "prettier": "prettier --write src/**/*.js",
    "compile": "babel src --out-dir .compiled --source-maps --watch",
    "apidoc": "apidoc -i ./src/server -o apidoc/",
    "check": "npm-check"
  },
  "devDependencies": {
    "apidoc": "0.17.6",
    "axios-mock-adapter": "1.15.0",
    "babel-cli": "6.26.0",
    "babel-core": "6.26.3",
    "babel-eslint": "10.0.1",
    "babel-loader": "7",
    "babel-polyfill": "6.26.0",
    "babel-preset-es2015": "6.24.1",
    "babel-preset-es2016": "6.24.1",
    "babel-preset-es2017": "6.24.1",
    "babel-preset-flow": "6.23.0",
    "babel-preset-react": "6.24.1",
    "babel-preset-stage-2": "6.24.1",
    "chai": "4.2.0",
    "chai-http": "4.2.0",
    "coveralls": "3.0.2",
    "cross-env": "5.2.0",
    "enzyme": "3.7.0",
    "enzyme-adapter-react-16": "1.6.0",
    "enzyme-to-json": "3.3.4",
    "eslint": "5.7.0",
    "eslint-plugin-flow-vars": "0.5.0",
    "eslint-plugin-flowtype": "2.50.3",
    "eslint-plugin-import": "2.14.0",
    "eslint-plugin-jsx-a11y": "6.1.2",
    "eslint-plugin-react": "7.11.1",
    "eslint-watch": "4.0.2",
    "flow-bin": "0.83.0",
    "jest": "23.6.0",
    "jest-localstorage-mock": "2.2.0",
    "mocha": "5.2.0",
    "morgan": "1.9.1",
    "node-sass-chokidar": "1.3.4",
    "npm-check": "5.9.0",
    "npm-run-all": "4.1.3",

    "nyc": "13.1.0",
    "prettier": "1.14.3",
    "redux-devtools-extension": "2.13.5",
    "webpack": "4.20.2",
    "webpack-bundle-analyzer": "3.0.2",
    "webpack-cli": "3.1.2"
  }
