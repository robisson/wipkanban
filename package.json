  "scripts": {
    "build-css": "node-sass-chokidar public/assets/scss/ -o public/assets/css/",
    "watch-css": "npm run build-css && node-sass-chokidar public/assets/scss/ -o public/assets/css/ --recursive --output-style compressed --watch",
    "start-js": "react-scripts start",
    "start": "npm-run-all -p watch-css start-js",
    "server": "npm-run-all -p watch-css webpack babel-node-js",
    "webpack": "webpack --watch",
    "build": "npm run build-css && react-scripts build",
    "test:client": "react-scripts test --env=jsdom",
    "test:server": "mocha src/server/tests --compilers js:babel-core/register",
    "eject": "react-scripts eject",
    "babel-node-js": "nodemon --exec babel-node src/server/server.js --presets es2015,es2016,es2017,react,stage-2"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.0.3",
