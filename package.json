{
  "name": "kanban",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@fortawesome/fontawesome": "^1.1.8",
    "@fortawesome/fontawesome-free-solid": "^5.0.13",
    "@fortawesome/react-fontawesome": "^0.0.20",
    "@material-ui/core": "1.0.0",
    "@material-ui/icons": "1.0.0",
    "axios": "0.18.0",
    "bcrypt-nodejs": "0.0.3",
    "body-parser": "1.18.2",
    "bootstrap": "4.0.0-beta.2",
    "cookie-parser": "1.4.3",
    "cors": "2.8.4",
    "csrf": "3.0.6",
    "dotenv": "5.0.1",
    "express": "4.16.2",
    "immutability-helper": "2.6.0",
    "jsonwebtoken": "8.1.0",
    "mongoose": "5.0.3",
    "multer": "1.3.0",
    "prop-types": "15.6.0",
    "react": "16.3.1",
    "react-dnd": "2.5.4",
    "react-dnd-html5-backend": "2.5.4",
    "react-dom": "16.3.1",
    "react-redux": "5.0.6",
    "react-router": "4.2.0",
    "react-router-dom": "4.2.2",
    "redux": "3.7.2",
    "redux-thunk": "2.2.0",
    "uuid": "3.2.1"
  },
  "scripts": {
    "build-css": "node-sass-chokidar public/assets/scss/ -o public/assets/css/",
    "watch-css": "npm run build-css && node-sass-chokidar public/assets/scss/ -o public/assets/css/ --recursive --output-style compressed --watch",
    "start-js": "react-scripts start",
    "start": "npm-run-all -p watch-css start-js",
    "server": "npm-run-all -p watch-css lint:watch webpack babel-node-js",
    "webpack": "webpack --watch",
    "flow": "flow check",
    "build": "npm run build-css && react-scripts build",
    "test": "cross-env CI=true react-scripts test --env=jsdom --coverage",
    "test:coverage": "cat ./coverage/lcov.info | coveralls",
    "babel-node-js": "nodemon --exec babel-node src/server/server.js",
    "lint": "esw webpack.config.* src --color",
    "lint:watch": "npm run lint -- --watch",
    "analyze": "webpack --profile --json > stats.json && webpack-bundle-analyzer stats.json -m server",
    "prettier": "prettier --write src/**/*.js",
    "compile": "babel src --out-dir .compiled --source-maps --watch",
    "apidoc": "apidoc -i ./src/server -o apidoc/",
    "check": "npm-check"
  },
  "devDependencies": {
    "apidoc": "0.17.6",
    "axios-mock-adapter": "1.10.0",
    "babel-cli": "6.26.0",
    "babel-core": "6.26.0",
    "babel-eslint": "8.0.3",
    "babel-loader": "7.1.2",
    "babel-polyfill": "6.26.0",
    "babel-preset-es2015": "6.24.1",
    "babel-preset-es2016": "6.24.1",
    "babel-preset-es2017": "6.24.1",
    "babel-preset-flow": "6.23.0",
    "babel-preset-react": "6.24.1",
    "babel-preset-stage-2": "6.24.1",
    "chai": "4.1.2",
    "chai-http": "4.0.0",
    "coveralls": "3.0.0",
    "cross-env": "5.1.1",
    "enzyme": "3.2.0",
    "enzyme-adapter-react-16": "1.0.4",
    "eslint": "4.13.1",
    "eslint-plugin-flow-vars": "0.5.0",
    "eslint-plugin-flowtype": "2.46.1",
    "eslint-plugin-import": "2.8.0",
    "eslint-plugin-jsx-a11y": "6.0.3",
    "eslint-plugin-react": "7.5.1",
    "eslint-watch": "3.1.3",
    "flow-bin": "0.69.0",
    "jest-localstorage-mock": "2.1.0",
    "mocha": "5.0.5",
    "morgan": "1.9.0",
    "node-sass-chokidar": "1.2.2",
    "npm-check": "5.5.2",
    "npm-run-all": "4.0.2",
    "nyc": "11.3.0",
    "prettier": "1.9.2",
    "react-scripts": "1.0.11",
    "redux-devtools-extension": "2.13.2",
    "webpack": "4.16.0",
    "webpack-bundle-analyzer": "2.13.1",
    "webpack-cli": "3.0.8"
  }
}
